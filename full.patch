+func hostnamesToStringListWithWildcard(h []k8s.Hostname) []string {
+	if len(h) == 0 {
+		return []string{"*"}
+	}
+	res := make([]string, 0, len(h))
+	for _, i := range h {
+		res = append(res, string(i))
+	}
+	return res
+}
