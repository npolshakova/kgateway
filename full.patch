-func visitFields(msg protoreflect.Message, ancestor_sensitive bool) {
-	msg.Range(func(fd protoreflect.FieldDescriptor, v protoreflect.Value) bool {
-		sensitive := ancestor_sensitive || isSensitive(fd)
