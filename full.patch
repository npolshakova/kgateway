+
+			proto, tlsConfig, ok := syncer.getProtocolAndTLSConfig(tc.gateway)
+
+			assert.Equal(t, tc.expectedOk, ok)
+			if tc.expectedOk {
+				assert.Equal(t, tc.expectedProto, proto)
