-		if xds == nil {
-			return
-		}
-		cu := sets.New[ConfigKey]()
-		for _, e := range events {
-			for _, i := range e.Items() {
-				c := f(i)
-				if c != (ConfigKey{}) {
-					cu.Insert(c)
-				}
-			}
-		}
-		if len(cu) == 0 {
-			return
-		}
-		xds.ConfigUpdate(&PushRequest{
-			Full:           true,
-			ConfigsUpdated: cu,
-			Reason:         NewReasonStats(ConfigUpdate),
-		})
-	}
