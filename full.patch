+							t.Logf("failed to unmarshal service: %v", err)
+							continue
+						}
+						address.Type = &api.Address_Service{Service: service}
+					} else if workloadData, ok := typeData["Workload"].(map[string]interface{}); ok {
+						workloadJSON, err := json.Marshal(workloadData)
+						if err != nil {
