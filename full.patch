+func augmentPortMatch(routes []*istio.HTTPRoute, port k8s.PortNumber) []*istio.HTTPRoute {
+	res := make([]*istio.HTTPRoute, 0, len(routes))
+	for _, r := range routes {
+		r = r.DeepCopy()
+		for _, m := range r.Match {
+			m.Port = uint32(port)
+		}
+		if len(r.Match) == 0 {
+			r.Match = []*istio.HTTPMatchRequest{{
+				Port: uint32(port),
+			}}
+		}
+		res = append(res, r)
+	}
+	return res
+}
