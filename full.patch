-	for _, s := range objectStrs {
-		if len(strings.TrimSpace(s)) == 0 {
-			continue
-		}
-		o, _, err := decode([]byte(s), nil, nil)
-		if err != nil {
-			return nil, fmt.Errorf("failed deserializing kubernetes object: %v (%v)", err, s)
-		}
-		objects = append(objects, o)
-	}
-	return objects, nil
-}
