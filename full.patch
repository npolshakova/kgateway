+		svc := &api.Service{
+			Name:      s.Name,
+			Namespace: s.Namespace,
+			Hostname:  string(InferenceHostname(s.Name, s.Namespace, a.DomainSuffix)),
+			Ports:     ports,
+		}
+
+		selector := make(map[string]string, len(s.Spec.Selector))
+		for k, v := range s.Spec.Selector {
+			selector[string(k)] = string(v)
+		}
