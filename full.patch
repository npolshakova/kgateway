+							Key:  "test-key",
+							Port: 8080,
+						},
+					},
+				},
+				Gateway: types.NamespacedName{Name: "test", Namespace: "default"},
+			},
+			expected: true,
+		},
+		{
+			name: "Different gateway",
+			resource1: ADPResource{
+				Resource: &api.Resource{
+					Kind: &api.Resource_Bind{
+						Bind: &api.Bind{
+							Key:  "test-key",
+							Port: 8080,
+						},
+					},
+				},
+				Gateway: types.NamespacedName{Name: "test", Namespace: "default"},
